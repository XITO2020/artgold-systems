      "icons": [{ "src": "/icons/upload-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "My Gallery",
      "short_name": "Gallery",
      "description": "View your artwork collection",
      "url": "/dashboard",
      "icons": [{ "src": "/icons/gallery-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Exchange",
      "short_name": "Exchange",
      "description": "Exchange your tokens",
